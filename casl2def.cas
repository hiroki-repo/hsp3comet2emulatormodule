RPUSH	.DEFINE
	PUSH #0,GR1
	PUSH #0,GR2
	PUSH #0,GR3
	PUSH #0,GR4
	PUSH #0,GR5
	PUSH #0,GR6
	PUSH #0,GR7
	.ENDDEF

RPOP	.DEFINE
	POP GR7
	POP GR6
	POP GR5
	POP GR4
	POP GR3
	POP GR2
	POP GR1
	.ENDDEF

IN	.DEFINE
	PUSH 0,GR1
	PUSH 0,GR2
	LAD GR1,%0%
	LAD GR2,%1%
	SVC 1
	POP GR2
	POP GR1
	.ENDDEF

OUT	.DEFINE
	PUSH 0,GR1
	PUSH 0,GR2
	LAD GR1,%0%
	LAD GR2,%1%
	SVC 2
	POP GR2
	POP GR1
	.ENDDEF
